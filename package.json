{
  "name": "expo-react-native-web-boilerplate",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev:ios": "eas build -p ios --profile development",
    "build:dev:device:ios": "eas build -p ios --profile development-device --local",
    "build:dev:android": "eas build -p android --profile development",
    "build:test:ios": "eas build -p ios --profile preview",
    "build:prod:ios": "eas build -p ios --profile production",
    "doctor": "npx expo-doctor --verbose",
    "doctor:fix": "npx expo install --check",
    "env:list": "eas env:list",
    "env:create": "eas env:create",
    "env:delete": "eas env:delete"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "@rneui/base": "^4.0.0-rc.8",
    "@rneui/themed": "^4.0.0-rc.8",
    "expo": "^53.0.0",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-localization": "^16.1.6",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1305.0",
    "react-native-popover-view": "^5.1.8",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "@rneui/base",
          "@rneui/themed"
        ]
      }
    }
  }
}
