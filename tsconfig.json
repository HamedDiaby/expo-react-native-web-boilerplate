{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
      "@components": ["src/components/index"],
      "@screens": ["src/screens/index"],
      "@navigation": ["src/navigation/index"],
      "@contexts": ["src/contexts/index"],
      "@themes": ["src/themes/index"],
    },
    "module": "ESNext"
  }
}
